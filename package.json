{
  "name": "@servicebus/trace",
  "version": "0.0.0-development",
  "description": "servicebus middleware to publish message receipt information to a central store, for message tracking and tracing purposes. ",
  "main": "index.js",
  "scripts": {
    "test": "make test",
    "format": "prettier-standard 'lib/**/*.js' 'test/**/*.js' 'bin/*.js' 'index.js'"
  },
  "bin": {
    "servicebus-trace": "./bin/trace.js"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test"
    }
  },
  "keywords": [
    "servicesbus",
    "distributed",
    "messaging",
    "tracking",
    "trace",
    "tracing"
  ],
  "author": "mattwalters5@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "husky": "^1.3.1",
    "jest": "^24.7.0",
    "lint-staged": "^8.1.5",
    "mocha": "^5.2.0",
    "prettier-standard": "^9.1.1",
    "servicebus": "^2.0.1",
    "should": "^13.2.3"
  },
  "dependencies": {
    "cli-table3": "^0.5.0",
    "colors.js": "^1.2.4",
    "debug": "^4.1.0",
    "hiredis": "^0.5.0",
    "lodash.get": "^4.1.2",
    "redis": "^2.4.2"
  }
}
